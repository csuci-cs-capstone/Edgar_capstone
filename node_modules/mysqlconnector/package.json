{
  "name": "mysqlconnector",
  "version": "1.0.21",
  "description": "MySQL connector",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pteyssedre/mysql-connector.git"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage",
      ".nyc_output",
      "index.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "keywords": [
    "mysql",
    "connector",
    "typescript",
    "async",
    "await",
    "transaction",
    "dbcontext"
  ],
  "author": {
    "name": "Pierre Teyssedre",
    "email": "pierre@teyssedre.ca"
  },
  "license": "MIT",
  "dependencies": {
    "mysql": "2.18.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^2.2.46",
    "@types/mysql": "^2.15.2",
    "chai": "^4.1.2",
    "coveralls": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
